<h1> Sign in to see course app!</h1>
<%= form_for @user, url: registrations_path do |f| %>

<%= f.label :user_name %>
<%= f.text_field :user_name %>

<%= f.label :about %>
<%= f.text_field :about %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation %>
<%= f.password_field :password_confirmation %>

<%= f.submit 'Sign Up' %>

<% end %>
